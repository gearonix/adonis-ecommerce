    "typescript": "4.9.5",
    "webpack": "^5.77.0"
  },
  "loki": {
    "configurations": {
      "chrome.laptop": {
        "target": "chrome.app",
        "width": 1920,
        "height": 1080,
        "deviceScaleFactor": 1,
        "mobile": false
      }
    }
  },
  "pnpm": {
    "allowDeprecated": {
      "fsevents": "*",
      "request": "2"
    }
  }
}
