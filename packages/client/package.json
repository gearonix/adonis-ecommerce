{
  "name": "client",
  "productName": "Adonis",
  "version": "0.1.0",
  "private": true,
  "main": ".electron/index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "storybook": "start-storybook -p 6006",
    "loki": "npx loki test && npm run visual_report",
    "loki:approve": "npx loki approve",
    "loki:build": "loki --requireReference --reactUri file:./storybook-static",
    "storybook:build": "build-storybook",
    "test": "jest",
    "visual_report": "node scripts/generate-visual-storybook-report/script.js && reg-cli --from .loki/report.json --report .loki/report.html",
    "test:watch": "jest --watch",
    "export": "next build && next export",
    "electron:start": "electron .",
    "electron:build": "cross-env NODE_ENV=electron-build npm run export && electron-builder",
    "electron:release": "electron-builder"
  },
  "build": {
    "asar": true,
    "files": [
      ".electron/index.js",
      "out"
    ],
    "win": {
      "icon": "public/assets/logo-mobile.png"
    },
    "publish": null
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@hookform/resolvers": "^2.9.11",
    "@mui/material": "^5.11.15",
    "@parvineyvazov/json-translator": "^1.6.3",
    "@react-oauth/google": "^0.6.1",
    "@react-spring/web": "^9.7.1",
    "@reduxjs/toolkit": "^1.9.3",
    "app-root-path": "^3.1.0",
    "axios": "^1.3.4",
    "classnames": "^2.3.2",
    "country-list-js": "^3.1.7",
    "dayjs": "^1.11.7",
    "electron-is-dev": "^1.2.0",
    "electron-next": "^3.1.5",
    "i18next": "^22.4.13",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.0",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.8",
    "react-i18next": "^12.2.0",
    "react-icons": "^4.8.0",
    "react-redux": "^8.0.5",
    "react-toastify": "^9.1.2",
    "react-use-measure": "^2.1.1",
    "socket.io-client": "^4.6.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.2.4",
    "@react-buddy/ide-toolbox": "^2.3.1",
    "@react-buddy/palette-mui": "^5.0.1",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-viewport": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/webpack": "^6.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/country-list-js": "^3.1.2",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@types/socket.io-client": "^3.0.0",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
